<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        package="kim.jeonghyeon.sample">

    <uses-permission android:name="android.permission.RECEIVE_BOOT_COMPLETED" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.INTERNET" />

    <!-- android:usesCleartextTraffic="true" is used for call http for sample api-->
    <application
            android:name="kim.jeonghyeon.sample.SampleApplication"
            android:allowBackup="true"
            android:icon="@mipmap/ic_launcher"
            android:label="@string/app_name"
            android:roundIcon="@mipmap/ic_launcher_round"
            android:supportsRtl="true"
            android:theme="@style/AppTheme"
            android:usesCleartextTraffic="true"
            tools:replace="android:name">
        <!--android:label="${app_name}"-->


        <activity android:name="kim.jeonghyeon.sample.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".viewmodel.startactivity.StartActivityActivity" />
        <activity android:name=".view.emptylayout.EmptyLayoutActivity" />

        <service
                android:name="kim.jeonghyeon.sample.etc.job.SampleJobIntentService"
                android:permission="android.permission.BIND_JOB_SERVICE" />
    </application>

</manifest>